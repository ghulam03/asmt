// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
   id      String @id @map("_id")
   uname  String
   password String

}

model Person {
  id      String @id @map("_id")
  uname   String
  email   String
  phone   Int
  company String
}
